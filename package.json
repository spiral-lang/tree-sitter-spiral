{
  "name": "tree-sitter-spiral",
  "version": "0.0.2",
  "description": "Spiral grammar for node-tree-sitter",
  "main": "index.js",
  "keywords": [
    "parser",
    "lexer"
  ],
  "author": "Cristian Lozano",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.14.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.16.2"
  },
  "scripts": {
    "test": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.spiral",
      "file-types": [
        "sp"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "locals": "queries/locals.scm",
      "injection-regex": "^(sp|spiral)$"
    }
  ]
}
