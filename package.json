{
  "name": "tree-sitter-spiral",
  "version": "0.0.3",
  "description": "Spiral grammar for node-tree-sitter",
  "main": "index.js",
  "keywords": [
    "parser",
    "spiral"
  ],
  "homepage": "https://splang.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/spiral-lang/tree-sitter-spiral.git"
  },
  "bugs": {
    "url": "https://github.com/spiral-lang/tree-sitter-spiral/issues"
  },
  "author": "Cristian Lozano <criloz@nebtex.com>",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.14.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.16.2"
  },
  "scripts": {
    "test": "tree-sitter test"
  },
  "tree-sitter": [
    {
      "scope": "source.spr",
      "file-types": [
        "spr",
        "spiral"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "locals": "queries/locals.scm",
      "injection-regex": "^(spr|spiral)$"
    }
  ]
}
